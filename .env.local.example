# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Neynar API Key (get from https://neynar.com/)
NEYNAR_API_KEY=your_neynar_key_here

# Upstash Redis (for caching and rate limiting)
UPSTASH_REDIS_REST_URL=your_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here

# Pinata IPFS (for frame images)
PINATA_API_KEY=your_pinata_key_here
PINATA_SECRET_KEY=your_pinata_secret_here

# Chainlink VRF (for random number generation)
VRF_COORDINATOR_ADDRESS=0x...
VRF_SUBSCRIPTION_ID=123

# Base RPC
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org
